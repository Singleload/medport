{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "vue/multi-word-component-names": ["error", {
      "ignores": ["App"]
    }],
    "vue/no-reserved-component-names": ["error", {
      "disallowVueBuiltInComponents": true
    }]
  },
  "globals": {
    "process": true
  }
}