ons {\n      margin-top: 2rem;\n      text-align: left;\n      background-color: $primary-lighter;\n      padding: 1.5rem;\n      border-radius: $border-radius;\n      \n      h2 {\n        font-size: 1.2rem;\n        color: $primary;\n        margin-bottom: 1rem;\n        text-align: center;\n      }\n      \n      ul {\n        list-style: none;\n        padding: 0;\n        display: grid;\n        grid-template-columns: repeat(2, 1fr);\n        gap: 0.5rem 2rem;\n        \n        @media (max-width: $breakpoint-sm) {\n          grid-template-columns: 1fr;\n        }\n        \n        li {\n          a {\n            display: flex;\n            align-items: center;\n            padding: 0.5rem;\n            color: $primary;\n            font-weight: 500;\n            transition: $transition-base;\n            border-radius: $border-radius-sm;\n            \n            &:hover {\n              background-color: rgba($primary, 0.1);\n              transform: translateX(5px);\n            }\n            \n            i {\n              margin-right: 0.5rem;\n            }\n          }\n        }\n      }\n    }\n  }\n}\n</style>"],"mappings":"AAkDA,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,OAAOA,CAAA,EAAG;IACR;IACAC,QAAQ,CAACC,KAAI,GAAI,wCAAwC;EAC3D;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}