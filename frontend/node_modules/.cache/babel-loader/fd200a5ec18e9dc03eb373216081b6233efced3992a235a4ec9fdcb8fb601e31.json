{"ast":null,"code":"export default {\n  name: 'ServiceList',\n  props: {\n    services: {\n      type: Array,\n      required: true\n    }\n  },\n  setup() {\n    const calculateDiscount = (originalPrice, discountPrice) => {\n      return Math.round((1 - discountPrice / originalPrice) * 100);\n    };\n    const formatPrice = price => {\n      return new Intl.NumberFormat('sv-SE').format(price);\n    };\n    return {\n      calculateDiscount,\n      formatPrice\n    };\n  }\n};","map":{"version":3,"names":["name","props","services","type","Array","required","setup","calculateDiscount","originalPrice","discountPrice","Math","round","formatPrice","price","Intl","NumberFormat","format"],"sources":["/home/dennis/medport/frontend/src/components/home/ServiceList.vue"],"sourcesContent":["<template>\n    <div class=\"service-list\">\n      <div class=\"row\">\n        <div v-for=\"service in services\" :key=\"service.id\" class=\"service-item\">\n          <div class=\"service-card\">\n            <div class=\"service-image\">\n              <img :src=\"service.image\" :alt=\"service.name\" />\n              <div v-if=\"service.discountedPrice\" class=\"discount-badge\">\n                -{{ calculateDiscount(service.price, service.discountedPrice) }}%\n              </div>\n            </div>\n            <div class=\"service-info\">\n              <h3>{{ service.name }}</h3>\n              <p>{{ service.shortDescription }}</p>\n              <div class=\"service-price\">\n                <span v-if=\"service.discountedPrice\" class=\"original-price\">{{ formatPrice(service.price) }} kr</span>\n                <span class=\"current-price\">{{ formatPrice(service.discountedPrice || service.price) }} kr</span>\n                <span v-if=\"service.isSubscription\" class=\"subscription-interval\">{{ service.subscriptionInterval }}</span>\n              </div>\n              <div class=\"service-actions\">\n                <router-link :to=\"`/service/${service.id}`\" class=\"btn btn-primary\">LÃ¤s mer</router-link>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    name: 'ServiceList',\n    props: {\n      services: {\n        type: Array,\n        required: true\n      }\n    },\n    setup() {\n      const calculateDiscount = (originalPrice, discountPrice) => {\n        return Math.round((1 - discountPrice / originalPrice) * 100);\n      };\n      \n      const formatPrice = (price) => {\n        return new Intl.NumberFormat('sv-SE').format(price);\n      };\n      \n      return {\n        calculateDiscount,\n        formatPrice\n      };\n    }\n  };\n  </script>\n  \n  <style lang=\"scss\" scoped>\n  .service-list {\n    .row {\n      display: flex;\n      flex-wrap: wrap;\n      margin: 0 -1rem;\n    }\n    \n    .service-item {\n      flex: 0 0 100%;\n      max-width: 100%;\n      padding: 0 1rem;\n      margin-bottom: 2rem;\n      \n      @media (min-width: $breakpoint-sm) {\n        flex: 0 0 50%;\n        max-width: 50%;\n      }\n      \n      @media (min-width: $breakpoint-lg) {\n        flex: 0 0 33.333333%;\n        max-width: 33.333333%;\n      }\n    }\n    \n    .service-card {\n      height: 100%;\n      border-radius: $border-radius;\n      overflow: hidden;\n      box-shadow: $box-shadow-sm;\n      background-color: white;\n      transition: transform 0.3s ease, box-shadow 0.3s ease;\n      \n      &:hover {\n        transform: translateY(-5px);\n        box-shadow: $box-shadow;\n      }\n      \n      .service-image {\n        position: relative;\n        height: 200px;\n        overflow: hidden;\n        \n        img {\n          width: 100%;\n          height: 100%;\n          object-fit: cover;\n        }\n        \n        .discount-badge {\n          position: absolute;\n          top: 10px;\n          right: 10px;\n          background-color: $primary;\n          color: white;\n          padding: 0.25rem 0.5rem;\n          border-radius: 0.25rem;\n          font-weight: bold;\n          font-size: 0.9rem;\n        }\n      }\n      \n      .service-info {\n        padding: 1.5rem;\n        \n        h3 {\n          margin-bottom: 0.5rem;\n          font-size: 1.25rem;\n        }\n        \n        p {\n          color: #6c757d;\n          margin-bottom: 1rem;\n        }\n        \n        .service-price {\n          margin-bottom: 1.5rem;\n          \n          .original-price {\n            text-decoration: line-through;\n            color: #6c757d;\n            margin-right: 0.5rem;\n            font-size: 0.9rem;\n          }\n          \n          .current-price {\n            font-weight: bold;\n            font-size: 1.2rem;\n            color: $primary;\n          }\n          \n          .subscription-interval {\n            display: block;\n            font-size: 0.9rem;\n            color: #6c757d;\n            margin-top: 0.25rem;\n          }\n        }\n        \n        .service-actions {\n          display: flex;\n          justify-content: center;\n        }\n      }\n    }\n  }\n  </style>"],"mappings":"AA8BE,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,iBAAgB,GAAIA,CAACC,aAAa,EAAEC,aAAa,KAAK;MAC1D,OAAOC,IAAI,CAACC,KAAK,CAAC,CAAC,IAAIF,aAAY,GAAID,aAAa,IAAI,GAAG,CAAC;IAC9D,CAAC;IAED,MAAMI,WAAU,GAAKC,KAAK,IAAK;MAC7B,OAAO,IAAIC,IAAI,CAACC,YAAY,CAAC,OAAO,CAAC,CAACC,MAAM,CAACH,KAAK,CAAC;IACrD,CAAC;IAED,OAAO;MACLN,iBAAiB;MACjBK;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}